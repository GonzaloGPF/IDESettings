<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl #10000d1" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl #10000c7" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl #10000c7" />
  </action>
  <action id="FileChooser.TogglePathShowing" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl less" />
  </action>
  <action id="ParameterInfo" />
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="alt u" />
  </action>
</keymap>