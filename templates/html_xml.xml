    </context>
  </template>
  <template name="vauto" value="&lt;v-auto-complete name=&quot;$NAME$&quot;&#10;                 ref=&quot;$NAME$&quot;&#10;                 url=&quot;$URL$&quot;&#10;                 url-attribute=&quot;$NAME$&quot;&#10;                 @input=&quot;onInput($event, '$NAME$')&quot;/&gt;" description="UI Element autocomplete field" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
