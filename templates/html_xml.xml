  </template>
  <template name="vtext" value="&lt;v-text :value=&quot;form.$NAME$&quot; &#10;        name=&quot;$NAME$&quot; &#10;        @input=&quot;onInput($event, '$NAME$')&quot;/&gt;" description="UI Element input field" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="vnumber" value="&lt;v-number name=&quot;$NAME$&quot; &#10;          :value=&quot;form.$NAME$&quot; &#10;          @input=&quot;onInput($event, '$NAME$')&quot;/&gt;" description="UI Element number field" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="vswitch" value="&lt;v-switch :value=&quot;form.$NAME$&quot; &#10;          name=&quot;$NAME$&quot; &#10;          @input=&quot;onInput($event, '$NAME$')&quot;/&gt;" description="UI Element switch field" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
