    </context>
  </template>
  <template name="vdate" value="&lt;v-date name=&quot;$NAME$&quot;&#10;        :value=&quot;form.$NAME$&quot;&#10;        @input=&quot;onInput($event, '$NAME$')&quot;/&gt;" description="IU Element for a date" toReformat="false" toShortenFQNames="true">
