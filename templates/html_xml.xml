  </template>
  <template name="vselect" value="&lt;v-select :selected=&quot;form.$SELECTED$ ? form.$SELECTED$.id : null&quot;&#10;          name=&quot;$NAME$_id&quot;&#10;          url=&quot;$NAME$s&quot;         &#10;          @input=&quot;onInput($event, '$NAME$')&quot;/&gt;" description="IU Element for a select" toReformat="false" toShortenFQNames="true">
