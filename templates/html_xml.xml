  <template name="ibr" value="&lt;info-block-row :label=&quot;$t('validation.attributes.$ATTRIBUTE$')&quot;&#10;                :value=&quot;data.$ATTRIBUTE$&quot;/&gt;" description="Info Block Row" toReformat="false" toShortenFQNames="true">
    <variable name="ATTRIBUTE" expression="" defaultValue="" alwaysStopAt="true" />
